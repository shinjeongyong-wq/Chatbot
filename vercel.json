{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "api/chat.js": {
            "memory": 1024,
            "maxDuration": 30
        },
        "api/notion.js": {
            "memory": 1024,
            "maxDuration": 60
        }
    }
}